<Project Sdk="Microsoft.NET.Sdk.Web">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net7.0</TargetFramework>
        <AssemblyName>Aiursoft.AiurDrive</AssemblyName>
        <RootNamespace>Aiursoft.AiurDrive</RootNamespace>
        <IsTestProject>false</IsTestProject>
        <IsPackable>false</IsPackable>
        <ImplicitUsings>enable</ImplicitUsings>
        <Description>AiurDrive is a file store for all kinds of users.</Description>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Aiursoft.Identity" Version="7.0.1" />
    </ItemGroup>
</Project>